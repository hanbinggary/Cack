page {
  width: 100%;
  height: 100%;
  color: #000;
  background: #f5f5f5;
}


/* 按钮 */
.btn-group {
  margin-top: 32px;
  display: flex;
  justify-content: flex-end;
}

.btn-group>.btn {
  width: 172rpx;
  padding: 8px;
  border-radius: 50px;
  text-align: center;
}

.btn-group>.btn:not(:last-child) {
  margin-right: 16px;
}

.cancal-btn {
  background: #fff;
  box-shadow: 0 1px 8px rgba(0, 0, 0, .1);
}

.cancal-btn:active {
  background-color: rgb(247, 247, 247);
}

.next-btn {
  color: #fff;
  background: #1890ff;
  box-shadow: 0 1px 8px rgba(24, 143, 255, 0.8);
}

.next-btn:active {
  background-color: #1890ffcc;
}

/* 空列表 */
.empty-list {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 200rpx;
  border-radius: 6px;
  background: #eee;
  color: #999;
  font-size: 28rpx;
}

